@page "/about"

<!DOCTYPE html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>About Us</title>
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

</head>

<body>

    <!-- Start Breadcrumbs -->
    <div style="width: 1450px; height: 300px; background: #E3D5FA">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-6">
                    <div class="breadcrumbs-content">

                        <h1 class="page-title">About Us</h1>
                        <br />
                        <br />
                        <p>
                            Welcome to Jobstacle, <br />
                            where we believe that finding the right job or the perfect candidate shouldn't be an obstacle course,
                            but rather a seamless journey towards professional fulfillment.
                        </p>
                        <br />
                        <br />
                    </div>
                </div>
                <div class="col-md-6">
                    <img src="/css/ppl.png" alt="Logo" style="width: 350px; height: 350px;margin-left: 200px;">
                </div>
            </div>
        </div>
    </div>
    <!-- End Breadcrumbs -->

    <br />
    <br />

    <!-- Start Content Section -->
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-lg-6 col-md-10 col-12">
                <div class="content-left">
                    <div class="row">
                        <div class="col-lg-6 col-12">
                            <img class="single-img mr-20" src="/css/pic.png" alt="#">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-10 col-12">
                <div class="content-right">
                    <h2>
                        Tailoring Your Success,<br>
                        Connecting You with the Perfect Fit
                    </h2>
                    <br />
                    <div class="single-list">
                        <div class="list-bod">
                            <div style="display: flex; align-items: center;">
                                <img src="/css/icon1.png" alt="Logo" style="width: 50px; height: 50px;">
                                <h5 style="margin-left: 10px;">#1 Jobs site in Singapore</h5>
                            </div>
                            <p style="margin-left: 60px;">Leading Singapore job board, connecting talent and opportunities for unparalleled career success.</p>
                        </div>
                    </div>
                    <br />
                    <div class="single-list">
                        <div class="list-bod">
                            <div style="display: flex; align-items: center;">
                                <img src="/css/icon2.png" alt="Logo" style="width: 50px; height: 50px;">
                                <h5 style="margin-left: 10px;">Seamless searching</h5>
                            </div>
                            <p style="margin-left: 60px;">Effortless job exploration with intuitive features, ensuring a smooth and tailored search experience.</p>
                        </div>
                    </div>
                    <br />
                    <div class="single-list">
                        <div class="list-bod">
                            <div style="display: flex; align-items: center;">
                                <img src="/css/icon3.png" alt="Logo" style="width: 50px; height: 50px;">
                                <h5 style="margin-left: 10px;">Hired in top companies</h5>
                            </div>
                            <p style="margin-left: 60px;">Jobstacle propels careers by facilitating placements in prestigious companies, unlocking professional excellence.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Content Section -->

    <br />
    <br />

    <!-- Start Apply Process Area -->
    <section class="apply-process section">
        <div style="width: 1450px; height: 250px; background: #E3D5FA">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-12">
                        <div class="process-item">
                            <br />
                            <br />
                            <img src="/css/icon4.png" alt="Logo" style="width: 50px; height: 50px;margin-bottom:10px;">
                            <br />
                            <h4>Register Your Account</h4>
                            <p>
                                Join the Jobstacle community by creating your account,
                                unlocking a world of tailored job opportunities that match your unique skills and career goals.
                            </p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-12">
                        <div class="process-item">
                            <br />
                            <br />
                            <img src="/css/icon5.png" alt="Logo" style="width: 50px; height: 50px; margin-bottom:10px;">
                            <br />
                            <h4>Upload Your Resume</h4>
                            <p>
                                Showcase your professional journey effortlessly by uploading your resume,
                                allowing employers to discover the full scope of your qualifications and experience at a glance.
                            </p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-12">
                        <div class="process-item">
                            <br />
                            <br />
                            <img src="/css/icon6.png" alt="Logo" style="width: 50px; height: 50px;margin-bottom:10px;">
                            <br />
                            <h4>Apply for Your Dream Job</h4>
                            <p>
                                Seize the opportunity to turn your career aspirations into reality by easily applying for your dream job through Jobstacle,
                                where each application is a step closer to overcoming your job search obstacles.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Apply Process Area -->

    <br />
    <br />

    <!-- Start Clients Area -->
    <div class="client-logo-section">
        <div class="container">
            <div class="client-logo-wrapper">
                <div class="client-logo-carousel d-flex align-items-center justify-content-between">
                    <div class="client-logo">
                        <img src="./css/c1.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c2.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c3.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c4.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c5.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c6.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c7.png" alt="#">
                    </div>
                    <div class="client-logo">
                        <img src="./css/c8.png" alt="#">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Clients Area -->

    <br />
    <br />

    <!-- Start Footer Area -->
    <footer class="footer">
        <div class="footer-middle">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-6 col-12">
                        <div class="f-about single-footer">
                            <div class="logo">
                                <img src="./css/logo.png" alt="Logo" style="width: 200px; height: 150px;margin-left:20px;">
                            </div>
                            <ul class="contact-address">
                                <li><span>Address:</span> 123 Career Avenue, Opportunity District</li>
                                <li><span>Email:</span> jobstacle@gmail.com</li>
                                <li><span>Call:</span> +65 1234 5678</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-8 col-12">
                        <div class="row">
                            <div class="col-lg-4 col-md-6 col-12">
                                <div class="single-footer f-link">
                                    <br />
                                    <br />
                                    <h3>For Candidates</h3>
                                    <ul>
                                        <li><a href="jobs">Jobs</a> </li>
                                        <li><a href="jobapplications">Job Applications</a></li>
                                        <li><a href="courses">Courses</a></li>
                                        <li><a href="courseregistrations">Course Registrations</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12">
                                <div class="single-footer f-link">
                                    <br />
                                    <br />
                                    <h3>For Employers</h3>
                                    <ul>
                                        <li><a href="companies">Companies</a></li>
                                        <li><a href="staffs">Staffs</a></li>
                                        <li><a href="organizers">Organizers</a></li>
                                        <li><a href="jobseekers">Job Seekers</a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-6 col-12">
                                <div class="single-footer newsletter">
                                    <br />
                                    <br />
                                    <h3>Join Our Newsletter</h3>
                                    <p>Subscribe to get the latest jobs posted, candidates...</p>

                                    <input name="EMAIL" placeholder="Your email address" class="common-input"
                                           onfocus="this.placeholder = ''"
                                           onblur="this.placeholder = 'Your email address'" required="" type="email">
                                    <div class="button">
                                        <button class="btn btn-dark" onclick="showPopup()" style="background-color:black; color: white;">Subscribe Now! <span class="dir-part"></span></button>

                                        <script>
                                            function showPopup() {
                                                alert("Thank you for subscribing!");
                                            }
                                        </script>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
